{
  "name": "@visuallangium/reactapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@typefox/monaco-editor-react": "1.0.0-next.14",
    "@visuallangium/langium-hat": "file:../langium-hat",
    "graphlib-dot": "^0.6.4",
    "graphviz-react": "^1.2.5",
    "langium": "^1.2.0",
    "langium-statemachine-dsl": "^1.2.0",
    "monaco-editor-workers": "^0.37.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-graph-vis": "^1.0.7",
    "react-scripts": "5.0.1",
    "vscode-languageclient": "~8.0.2",
    "vscode-languageserver": "~8.0.2",
    "web-vitals": "^2.1.4"
  },
  "volta": {
    "node": "18.16.0",
    "npm": "9.6.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "copy:sc": "npx shx cp ../../node_modules/langium-statemachine-dsl/language-configuration.json ./public/ls/statemachine-configuration.json && npx shx cp ../../node_modules/langium-statemachine-dsl/syntaxes/statemachine.tmLanguage.json ./public/ls/statemachine-grammar.json",
    "build:worker": "esbuild ../../node_modules/langium-statemachine-dsl/out/language-server/main-browser.js --bundle --format=iife --outfile=./public/ls/statemachineServerWorker.js",
    "build:langium": "esbuild ./src/langium-worker.ts --bundle --format=iife --outfile=./public/ls/langiumServerWorker.js",
    "build": "npm run copy:sc && npm run build:worker && npm run build:langium && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.2.7",
    "esbuild": "^0.17.19",
    "typescript": "^4.9.5"
  }
}
